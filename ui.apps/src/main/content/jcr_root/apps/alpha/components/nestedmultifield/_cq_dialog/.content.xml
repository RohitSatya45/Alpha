<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Nested Multifield Dialog"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <Author_x0020_Details
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Author Details"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <authorfullname
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Full Name"
                                                name="./authorfullname"/>
                                            <well
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/well">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <authorbooksdetails
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                        composite="{Boolean}true"
                                                        fieldLabel="Author Book Details">
                                                        <field
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                                            name="./authorbooksdetails">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <bookname
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    emptyText="enter book name"
                                                                    fieldLabel="Book Name"
                                                                    name="./bookname"/>
                                                                <bookpublishedyear
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="/libs/granite/ui/components/coral/foundation/form/datepicker"
                                                                    fieldLabel="Published Year"
                                                                    name="./bookpublishedyear"/>
                                                                <noofcopies
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/numberfield"
                                                                    fieldLabel="No Of Copies"
                                                                    name="./noofcopies"/>
                                                                <bookeditions
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                    composite="{Boolean}true"
                                                                    fieldLabel="Book Editions">
                                                                    <field
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                        name="./bookeditions">
                                                                        <items jcr:primaryType="nt:unstructured">
                                                                            <bookeditionname
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                emptyText="enter book name"
                                                                                fieldLabel="Book Edition Name"
                                                                                name="./bookeditionname"/>
                                                                            <editionpublishedyear
                                                                                jcr:primaryType="nt:unstructured"
                                                                                sling:resourceType="/libs/granite/ui/components/coral/foundation/form/datepicker"
                                                                                fieldLabel="Edition Published Year"
                                                                                name="./editionpublishedyear"/>
                                                                        </items>
                                                                    </field>
                                                                </bookeditions>
                                                            </items>
                                                        </field>
                                                    </authorbooksdetails>
                                                </items>
                                            </well>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </Author_x0020_Details>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
